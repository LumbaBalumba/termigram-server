generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id          Int    @id @default(autoincrement())
  name        String
  second_name String
  login       String @unique
  password    String
}

model Mesage {
  id        Int    @id @default(autoincrement())
  text      String
  sender_id String
}
